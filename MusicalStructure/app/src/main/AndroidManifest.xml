<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Hymn"
            android:label="@string/Hymn"/>
        <activity
            android:name=".Everglow"
            android:label="@string/Everglow"/>
        <activity
            android:name=".Adventure"
            android:label="@string/Adventure"/>
        <activity
            android:name=".Amazing"
            android:label="@string/Amazing"/>
        <activity
            android:name=".Order"
            android:label="@string/Order"/>
    </application>

</manifest>